{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": {
    "public": "build/web",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "**",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache, no-store, must-revalidate"
          },
          {
            "key": "Pragma",
            "value": "no-cache"
          },
          {
            "key": "Expires",
            "value": "0"
          }
        ]
      },
      {
        "source": "**/*.js",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache, no-store, must-revalidate"
          }
        ]
      },
      {
        "source": "**/*.html",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-cache, no-store, must-revalidate"
          }
        ]
      }
    ]
  },
  "flutter": {
    "platforms": {
      "android": {
        "default": {
          "projectId": "mapa-gabinetes-hlcamadora",
          "appId": "1:483456372206:android:e3db4645241df0562152ff",
          "fileOutput": "android/app/google-services.json"
        }
      },
      "ios": {
        "default": {
          "projectId": "mapa-gabinetes-hlcamadora",
          "appId": "1:483456372206:ios:b32b796aa3e7ff1e2152ff",
          "uploadDebugSymbols": false,
          "fileOutput": "ios/Runner/GoogleService-Info.plist"
        }
      },
      "macos": {
        "default": {
          "projectId": "mapa-gabinetes-hlcamadora",
          "appId": "1:483456372206:ios:b32b796aa3e7ff1e2152ff",
          "uploadDebugSymbols": false,
          "fileOutput": "macos/Runner/GoogleService-Info.plist"
        }
      },
      "dart": {
        "lib/firebase_options.dart": {
          "projectId": "mapa-gabinetes-hlcamadora",
          "configurations": {
            "android": "1:483456372206:android:e3db4645241df0562152ff",
            "ios": "1:483456372206:ios:b32b796aa3e7ff1e2152ff",
            "macos": "1:483456372206:ios:b32b796aa3e7ff1e2152ff",
            "web": "1:483456372206:web:e8e4a9b67706c67f2152ff",
            "windows": "1:483456372206:web:0b3f7727418edae52152ff"
          }
        }
      }
    }
  }
}